{
	"name": "cTouch r2 (browserUA)",
	"description": "You can browse smartphone websites. No configuration screens; command line --user-agent value is used.",
	"version": "1.7.3.20824",
	"update_url": "http://ctouch.googlecode.com/hg/updates.xml",
	"homepage_url": "https://code.google.com/p/ctouch/",
	"content_scripts": [
	{
		"all_frames": true,
		"js": ["ctouch_bootstrap.js","ctouch_touch.js"],
		"matches": ["<all_urls>"],
		"run_at": "document_start"
	},
	{
		"all_frames": true,
		"js": ["ctouch_css.js"],
		"matches": ["<all_urls>"],
		"run_at": "document_end"
	}
	],
	"icons": {
		"128": "icon128.png",
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png"
	}
}
