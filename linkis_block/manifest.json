{
	"name": "LinkisBlock",
	"description": "Redirects linkis.com to original website.",
	"version": "0.0.0.1",
	"manifest_version": 2,
	"homepage_url": "https://github.com/cielavenir/ctouch/",
	"content_scripts": [
	{
		"js": ["linkisblock.user.js"],
		"matches": ["http://linkis.com/*"],
		"run_at": "document_end"
	}
	]
}
