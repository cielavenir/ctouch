{
	"name": "RedomeBlock",
	"description": "Redirects redo.me.uk to original website",
	"version": "0.0.0.1",
	"manifest_version": 2,
	"homepage_url": "https://github.com/cielavenir/ctouch/",
	"background": {
		"scripts": ["redome_redirect.js"]
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"*://redo.me.uk/*"
	],
	"icons": {
		"128": "icon128.png",
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png"
	}
}
