{
	"name": "cTouch r2 (external)",
	"description": "You can browse smartphone websites. Configuration is saved to external CGI.",
	"version": "2.1.1.30301",
	"manifest_version": 2,
	"update_url": "http://ctouch.googlecode.com/hg/updates.xml",
	"homepage_url": "https://code.google.com/p/ctouch/",
	"background": {
		"scripts": ["ctouch_saveconfig.js","ctouch_bg.js"]
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"management",
		"<all_urls>"
	],
	"options_page": "ctouch_option.html",
	"browser_action": {
		"default_icon": "icon16.png",
		"default_title": "cTouch r2 (external)",
		"default_popup": "ctouch_popup.html"
	},
	"content_scripts": [
	{
		"all_frames": true,
		"js": ["ctouch_bootstrap.js"],
		"matches": ["<all_urls>"],
		"run_at": "document_start"
	},
	{
		"all_frames": true,
		"js": ["ctouch_css.js"],
		"matches": ["<all_urls>"],
		"run_at": "document_end"
	}
	],
	"icons": {
		"128": "icon128.png",
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png"
	}
}
