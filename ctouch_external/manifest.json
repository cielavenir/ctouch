{
	"name": "cTouch r2 (external)",
	"description": "You can browse smartphone websites.",
	"version": "1.5.0",
	"manifest_version": 2,
	"background": {
		"scripts": ["ctouch_bg.js"]
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"browser_action": {
		"default_icon": "icon16.png",
		"default_popup": "ctouch_popup.html"
	},
	"content_scripts": [
	{
		"all_frames": true,
		"js": ["ctouch_bootstrap.js","ctouch_touch.js"],
		"matches": ["<all_urls>"],
		"run_at": "document_start"
	}
	],
	"icons": {
		"128": "icon128.png",
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png"
   }
}
